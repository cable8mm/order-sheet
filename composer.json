{
    "name": "cable8mm/order-sheet-invoice-generator",
    "description": "This package is able to generate a kind of seed for order sheets and order invoices so you can use it for testing like that",
    "keywords": [
        "cable8mm",
        "order-sheet-invoice-generator"
    ],
    "homepage": "https://github.com/cable8mm/order-sheet-invoice-generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Samgu Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.20",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Cable8mm\\OrderSheetInvoiceGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cable8mm\\OrderSheetInvoiceGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "lint": "./vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    }
}